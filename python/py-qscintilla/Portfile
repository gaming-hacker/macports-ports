PortSystem          1.0

name                py-qscintilla
version             2.10.1
revision            2
categories          python devel
license             GPL-3

if {${subport} eq ${name}} {
    replaced_by         py-qscintilla-qt4
    PortGroup           obsolete 1.0
}

set python_versions {27 36}
foreach pver ${python_versions} {
    subport py${pver}-qscintilla {
        replaced_by    py${pver}-qscintilla-qt4
        PortGroup      obsolete 1.0
    }
}
